{
  "name": "frontier-alpha",
  "version": "1.0.0",
  "description": "AI-Powered Cognitive Factor Intelligence Platform",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run client:dev\"",
    "build": "tsc && npm run client:build",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "client:install": "cd client && npm install",
    "ml:start": "cd ml && uvicorn main:app --reload --port 8000"
  },
  "keywords": [
    "fintech",
    "ai",
    "portfolio",
    "trading",
    "factors"
  ],
  "author": "Metaventions AI",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^10.0.1",
    "axios": "^1.6.5",
    "date-fns": "^3.3.1",
    "decimal.js": "^10.4.3",
    "fastify": "^4.26.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/pg": "^8.11.0",
    "@types/ws": "^8.5.10",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}
