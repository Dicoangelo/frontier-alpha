{
  "name": "frontier-alpha",
  "version": "1.0.4",
  "description": "AI-Powered Cognitive Factor Intelligence Platform",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file .env src/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run client:dev\"",
    "build": "tsc && npm run client:build",
    "build:server": "esbuild src/index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js --format=esm --packages=external",
    "start": "node --env-file .env dist/index.js",
    "test": "vitest",
    "test:unit": "vitest run src/**/*.test.ts",
    "test:e2e": "vitest run tests/e2e",
    "test:coverage": "vitest run --coverage",
    "test:all": "npm run test:unit && cd client && npm run test:run",
    "lint": "eslint src/",
    "client:dev": "cd client && npm run dev",
    "client:build": "cd client && npm run build",
    "client:install": "cd client && npm install",
    "client:test": "cd client && npm run test:run",
    "ml:start": "cd ml && uvicorn main:app --reload --port 8000",
    "download-ken-french": "tsx scripts/download-ken-french.ts",
    "db:migrate": "cd supabase && supabase db push",
    "db:reset": "cd supabase && supabase db reset"
  },
  "keywords": [
    "fintech",
    "ai",
    "portfolio",
    "trading",
    "factors"
  ],
  "author": "Metaventions AI",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^10.0.1",
    "@supabase/supabase-js": "^2.94.0",
    "axios": "^1.6.5",
    "date-fns": "^3.3.1",
    "decimal.js": "^10.4.3",
    "fastify": "^4.26.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.11.5",
    "@types/pg": "^8.11.0",
    "@types/ws": "^8.5.10",
    "@vercel/node": "^5.5.28",
    "adm-zip": "^0.5.10",
    "concurrently": "^9.2.1",
    "csv-parse": "^5.5.3",
    "esbuild": "^0.27.2",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^1.2.2"
  }
}
